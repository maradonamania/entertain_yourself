      <% if current_user.already_liked?(plan) %>
        <%= link_to image_tag("likes-icon.png" ,class:"favorite-star-icon",width:"20",height:"20" ),plan_like_path(plan.id,current_user.id),method: :delete %>
        <span>いいね <%= plan.likes.count %></span>
      <% else %>
        <%= link_to image_tag("likes-icon.png" ,class:"favorite-star-icon",width:"20",height:"20" ),plan_likes_path(plan.id),method: :post%>
        <span>いいね <%= plan.likes.count %></span>
        <% end %>